page {
  background-color: #EBEEF5;
  overflow: hidden;
}

.bottom_bar {
  background-color: #fff;
  position: absolute;
  bottom: 0px;
  height: 70px;
  width: 750rpx;
  display: flex;
  justify-content: space-between;
  z-index: 1;
}

.bottom_bar_btn {
  height: 40px;
  width: 200rpx;
  border-radius: 5px;
  background-color: #1296db;
  text-align: center;
  margin-right: 60rpx;
  line-height: 40px;
  color: #fff;
  margin-top: 15px;
  z-index: 1;
}

.file_add {
  width: 60px;
  height: 60px;
  font-size: small;
  position: absolute;
  right: 20rpx;
  bottom: 100px;
  text-align: center;
  z-index: 1;
}

.file_add image {
  width: 50px;
  height: 50px;
}

.file-list {
  z-index: -1;
  width: 700rpx;
  height: 280rpx;
  background-color: #fff;
  margin-top: 20rpx;
  border-radius: 5px;
  margin-left: 25rpx;

}

.file-info {
  display: flex;
}

.file-s-info {
  width: 150rpx;
  height: 180rpx;
  font-size: small;
  color: #303133;
  position: relative;
  top: 20rpx;
  left: 20rpx;
}

.file-info-extra {
  margin-top: 20rpx;
  font-size: small;
  margin-left: 20rpx;
  width: 440rpx;
  height: 180rpx;
  overflow: hidden;
}

.file-s-info-size {
  height: 41rpx;
  margin-top: 20rpx;
  margin-left: 40rpx;
  line-height: 41rpx;
}

.file-s-info-way {
  width: 126rpx;
  height: 72rpx;
  text-align: center;
  margin-left: 12rpx;
  line-height: 72rpx;
}

.file-s-info-suffix {
  font-size: small;
  width: 110rpx;
  height: 30rpx;
  border-radius: 0px 0px 10px 10px;
  height: 20px;
  font-weight: 300;
  margin-left: 20rpx;
  text-align: center;
  color: #fff;
  background-color: #707072;
}

.file-info-extra-color {
  width: 50px;
  height: 20px;
  background-color: #909399;
  color: #fff;
  text-align: center;
  border-radius: 5px;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 10rpx;
}

.file-info-extra-set {
  margin-top: 10rpx;
  width: 90px;
  text-align: center;
  color: #fff;
  background-color: #409EFF;
  height: 50rpx;
  line-height: 50px;
  border-radius: 5px;
  display: flex;
  font-weight: 300;
  line-height: 24px;
}

.file-info-extra-set image {
  width: 35rpx;
  height: 35rpx;
  margin-top: 4.5px;
  margin-left: 5px;
}

.file-info-extra-name {
  font-size: small;
  color: #303133;
  font-weight: 400;
  height: 50rpx;
  line-height: 50rpx;
}

.file-info-progress {
  width: 650rpx;
  height: 20rpx;
  background-color: #E6A23C;
  margin-left: 25rpx;
  margin-top: 10rpx;
  border-radius: 20px;
}

.file-info-page-count {
  color: #606266;
  font-size: small;
  font-weight: 400;
  display: flex;
  margin-top: 10rpx;
  text-align: center;
}

.file-info-delete {
  width: 100rpx;
  height: 100rpx;
}

.file-info-delete image {
  width: 60rpx;
  height: 60rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.printer-set {
  width: 750rpx;
  position: absolute;
  bottom: 0px;
  background-color: #fff;
  z-index: 100;
  border-radius: 10px 10px 0px 0px;
}

.printer-cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99;
}

.printer-param{
  height: 120rpx;
  line-height: 120rpx;
  color: #303133;
  font-weight: 500;
  margin-left: 20rpx;
  display: flex;
  justify-content: space-between;
}
.printer-param-view view{
  height: 70rpx;
  line-height: 70rpx;
  font-size: small;
  margin-top: 25rpx;
  margin-right: 40rpx;
  font-weight: 300;
  width: 70rpx;
  text-align: center;
  border-radius: 5px;
  
}
.printer-line{
  background-color: #DCDFE6;
  width: 730rpx;
  height: 1px;
  margin-left: 10rpx;
}
.printer-set-btn{
  position: absolute;
  bottom: 20px;
  height: 50px;
  line-height: 50px;
  width: 700rpx;
  border-radius: 5px;
  text-align: center;
  background: linear-gradient(to right, #54b3df, #4b9cdd);
  color: #fff;
  font-weight: 300;
  margin-left: 25rpx;
}
.printer-param-count{
  display: flex;
  border: 1px solid #303133;
  height: 70rpx;
  width: 250rpx;
  line-height: 70rpx;
  margin-top: 25rpx;
  margin-right: 40rpx;
}
.printer-param-count view{
  width: 75rpx;
  text-align: center;
  font-size: xx-large;
  font-weight: 400;
  color: #909399;

}
.printer-param-count input{
  height: 70rpx;
  width: 100rpx;
  text-align: center;
} 


.file-s-info {
  background-size: 150rpx 180rpx;
  background-image: url(data:image/png;base64,);
}