    <image src="/images/wxDoc.png" mode=""/>
  </view>
  <view class="printer-bar-text" >
    <view style="display: flex; flex-direction: column;justify-content:space-between;">
    <view></view>
      <view style="color: #0070FF; font-weight: 500; font-size: x-large;">图文打印</view>
    </view>
    <view style="margin-top: 10rpx; color:#303133; font-size: small;">
      wps | word | doc | docx |  pdf | 图片
    </view>
  </view>
</view>
<view class="printer-bar" >
  <view class="printer-bar-image">
    <image src="/images/wxDoc.png" mode=""/>
  </view>
  <view class="printer-bar-text">
    <view style="display: flex; flex-direction: column;justify-content:space-between;">
    <view></view>
      <view style="color: #0070FF; font-weight: 500; font-size: x-large;">
        照片冲印
      </view>
    </view>
    <view style="margin-top: 10rpx; color:#303133; font-size: small;">
      一寸 | 二寸 
    </view>
  </view>
</view>