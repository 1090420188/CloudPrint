<view class="printer-back">
    <image src="/images/back.png" mode=""/>
</view>
<view class="printer-info" >
  <view style="display: flex;  margin-left: 25px;margin-top: 50px;"> 
    <view>
      <image src="/images/shop.png" />
    </view>
    <view style="height: 75px; line-height: 75px; margin-left: 10px;color: #fff;">
      <view style="font-size: x-large; font-weight: 500;">图文打印</view>
    </view>
  </view>
</view>
<view class="printer-name">
  <view>
    <image src="/images/address.png" />
  </view>
  <view style="line-height: 50px;margin-left: 20rpx;color: #303133;font-size: small;font-weight: 300;">
    山西省阳泉市盂县
  </view>
</view>

<view class="printer-bar" style="margin-top: 70px;" bindtap="navtoaddfile">
